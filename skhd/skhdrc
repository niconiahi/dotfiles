# focus window
cmd - j : yabai -m window --focus south
cmd - k : yabai -m window --focus north
cmd - h : yabai -m window --focus west
cmd - l : yabai -m window --focus east

# focus space
cmd - 1 : yabai -m space --focus 1; yabai -m window --focus mouse
cmd - 2 : yabai -m space --focus 2; yabai -m window --focus mouse
cmd - 3 : yabai -m space --focus 3; yabai -m window --focus mouse
cmd - 4 : yabai -m space --focus 4; yabai -m window --focus mouse
cmd - 5 : yabai -m space --focus 5; yabai -m window --focus mouse
cmd - 6 : yabai -m space --focus 6; yabai -m window --focus mouse
cmd - 7 : yabai -m space --focus 7; yabai -m window --focus mouse
cmd - 8 : yabai -m space --focus 8; yabai -m window --focus mouse
cmd - 9 : yabai -m space --focus 9; yabai -m window --focus mouse
cmd - 0 : yabai -m space --focus 10; yabai -m window --focus mouse

# move window to space and focus
cmd + shift - 1 : yabai -m window --space 1
cmd + shift - 2 : yabai -m window --space 2
cmd + shift - 3 : yabai -m window --space 3
cmd + shift - 4 : yabai -m window --space 4
cmd + shift - 5 : yabai -m window --space 5
cmd + shift - 6 : yabai -m window --space 6
cmd + shift - 7 : yabai -m window --space 7
cmd + shift - 8 : yabai -m window --space 8
cmd + shift - 9 : yabai -m window --space 9
cmd + shift - 0 : yabai -m window --space 10
